{
  "name": "calculation-tool",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:preview": "compodoc -p tsconfig.doc.json -d src/assets/docs/frontend && ng serve",
    "build": "ng build",
    "build:preview": "compodoc -p tsconfig.doc.json -d src/assets/docs/frontend && ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "font": "svgtofont --sources ./src/assets/images/engify-icons --output ./src/assets/fonts/engify-icons",
    "compodoc": "compodoc -p tsconfig.doc.json -d src/assets/docs/frontend"
  },
  "svgtofont": {
    "fontName": "engify-icons",
    "classNamePrefix": "ei",
    "css": {
      "fontSize": "16px",
      "include": "scss"
    },
    "outSVGReact": false,
    "outSVGReactNative": false,
    "outSVGPath": false,
    "emptyDist": true
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.0.6",
    "@angular/common": "^18.0.6",
    "@angular/compiler": "^18.0.6",
    "@angular/core": "^18.0.6",
    "@angular/forms": "^18.0.6",
    "@angular/platform-browser": "^18.0.6",
    "@angular/platform-browser-dynamic": "^18.0.6",
    "@angular/router": "^18.0.6",
    "@ngrx/effects": "^18.0.1",
    "@ngrx/operators": "^18.0.0",
    "@ngrx/store": "^18.0.1",
    "@types/mathjs": "^9.4.2",
    "@types/uuid": "^9.0.8",
    "angular-i18next": "^18.0.0",
    "arrify": "^3.0.0",
    "color": "^4.2.3",
    "i18next": "^23.8.2",
    "i18next-browser-languagedetector": "^7.2.0",
    "i18next-http-backend": "^2.4.3",
    "lodash": "^4.17.21",
    "mathjs": "^12.4.1",
    "moment": "^2.29.4",
    "object-hash": "^3.0.0",
    "papaparse": "^5.4.1",
    "primeflex": "^3.3.1",
    "primeicons": "^7.0.0",
    "primeng": "~17.18.3",
    "rxjs": "~7.8.0",
    "svgtofont": "^4.2.0",
    "tslib": "^2.3.0",
    "type-fest": "^4.23.0",
    "uuid": "^9.0.1",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.0.7",
    "@angular/cli": "~18.0.7",
    "@angular/compiler-cli": "^18.0.6",
    "@angular/localize": "^18.0.6",
    "@compodoc/compodoc": "^1.1.26",
    "@types/color": "^3.0.6",
    "@types/jasmine": "~4.3.0",
    "@types/lodash": "^4.14.202",
    "@types/object-hash": "^3.0.6",
    "@types/papaparse": "^5.3.14",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~5.4.5"
  }
}